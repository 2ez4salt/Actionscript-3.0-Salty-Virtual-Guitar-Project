import flash.events.MouseEvent;

if( mi0.addEventListener(MouseEvent.CLICK, cal)) {
   		function cal(e:MouseEvent):void { 
var adres:URLRequest=new URLRequest("mi0.mp3");
//URLReguest adresi hafızada saklamak için kullanılır.
 
var ses:Sound=new Sound(adres);
			ses.play();


//Sound un sayfa içinde bir örneğini oluşturuyoruz.
 	}
	}  
	
